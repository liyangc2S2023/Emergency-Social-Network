.ui.borderless.fixed.menu#main-header
    .ui.container
        a.item(onclick="goBack()") 
            i.chevron.left.icon#main-page-back
        h1.ui.left.aligned.header#title #{pageView}
        a.right.item
            a.right.item#searchButton(onclick="displaySearch()") 
                .ui.icon
                    i.search.icon  
            a.right.item#speedTest(onclick="speedTestModalDisplayAndHideOther()") 
                .ui.icon#speedPopup(data-content="Speed Test")
                    i.bolt.icon  
            a.right.item#logout(href="/logout") logout
